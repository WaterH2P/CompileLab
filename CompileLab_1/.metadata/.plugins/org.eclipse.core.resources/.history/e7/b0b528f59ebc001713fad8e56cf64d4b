package symbol;

/**
 * @author: hzp
 * @date: 2017/10/29 上午11:20
 * @description:
 */
public class Token{
    private int row = -1;
    private String Tag = "Token";
    private String value = "";

    Token(int row, String Tag){
        this.row = row;
        this.Tag = Tag;
    }
    Token(int row, String Tag, String symbolFileName){
        this.row = row;
        this.Tag = Tag;
        
    }

    public int getRow(){
        return this.row;
    }
    public String getTag(){
        return this.Tag;
    }
    public String getValue(){
        return this.value;
    }

    public String showContent(){
        return this.row + " " + this.Tag + " " + this.value;
    }
}